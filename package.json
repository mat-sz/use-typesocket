    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "react": "^18.0.0",
